<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="30dp"
    tools:context=".AutoInsurActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="332dp"
        android:layout_height="282dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/taylorinsurancepuregreen" />
    <TextView
        android:id="@+id/txtMainAutoInsur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:text="Contact Us"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/txtContactInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtMainAutoInsur"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="Phone: 709-123-4567"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtContactInfoEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtContactInfo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="Email: quote@taylorinsurance.com"
        android:textSize="20sp" />

    <Button
        android:id="@+id/btnBackContactUs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:paddingHorizontal="80dp"
        android:paddingVertical="20dp"
        android:text="Home"
        app:icon="@android:drawable/ic_dialog_dialer" />
</RelativeLayout>